    <managed-bean-class>com.ibm.xsp.extlib.sbt.security.password.store.BANSFPasswordStore</managed-bean-class>
    <managed-bean-scope>application</managed-bean-scope>
    <managed-property>
      <property-name>database</property-name>
      <value>WebSecurityStore.nsf</value>
    </managed-property>
  </managed-bean>
